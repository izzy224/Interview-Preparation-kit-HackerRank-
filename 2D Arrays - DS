// Unfinished

#include <bits/stdc++.h>

using namespace std;

int hourglassSum(vector<vector<int>> x) {
  int val(0);
  for (int y = 0; y < x.size()-2 ; y++) {
    for (int i = 0; i < x[0].size()-2; i++) {
      int val2;
      val2 = x[i][y] + x[i + 1][y] + x[i + 2][y] + x[i + 1][y + 1] +x[i][y + 2] + x[i + 1][y +2]+ x[i + 2][y + 2];
      if (val2 > val)
        val = val2;
    }
  }

  return val;
}

int main()
{
    ofstream fout(getenv("OUTPUT_PATH"));

    vector<vector<int>> arr(6);
    for (int i = 0; i < 6; i++) {
        arr[i].resize(6);

        for (int j = 0; j < 6; j++) {
            cin >> arr[i][j];
        }

        cin.ignore(numeric_limits<streamsize>::max(), '\n');
    }



    fout << hourglassSum(arr) << "\n";

    fout.close();

    return 0;
}
